{
    "name": "phprealm/emulator",
    "description": "The Emulator for the open source MMORPG game called phprealm",
    "type": "project",
    "require": {
        "cboden/ratchet": "^0.4.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true
    },
    "authors": [
        {
            "name": "Milan WinQz"
        }
    ]
}